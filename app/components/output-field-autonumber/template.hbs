{{#if (or model.isNew (not value))}}
  <OutputText
    @value={{t 'ember-mist-components.labels.automatically_generated'}}
    @prefix={{@prefix}}
    @suffix={{@suffix}}
    @class={{@outputClass}}
    @options={{this.outputOptions}} />
{{else}}
  <OutputText
    @value={{this.computedValue}}
    @prefix={{@prefix}}
    @suffix={{@suffix}}
    @class={{@outputClass}}
    @options={{this.outputOptions}} />
{{/if}}