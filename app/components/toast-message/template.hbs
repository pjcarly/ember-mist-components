<div id={{this.toastId}} class={{this.class}} role="alert" aria-live={{this.ariaLive}} aria-atomic="true">
  <div class="toast-header">
    <strong class="toast-subject">{{@toast.subject}}</strong>
    {{!-- <small>11 mins ago</small> --}}
    <button type="button" class="btn btn-icon close" aria-label="Close" {{on "click" this.close}}>
      <IconClose />
    </button>
  </div>
  {{#if @toast.message}}
    <div class="toast-body">
      {{@toast.message}}
    </div>
  {{/if}}
</div>