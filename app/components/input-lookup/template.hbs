<span class={{this.computedClass}}>
  {{#if @prefix}}
    <div class="input-group-prepend">
      <span class="input-group-text prefix">
        {{@prefix}}
      </span>
    </div>
  {{/if}}
  <InputModelAutocomplete
    @modelName={{@modelName}}
    @value={{@value}}
    @inputId={{@inputId}}
    @placeholder={{@placeholder}}
    @valueChanged={{this.valueSelected}}
    @options={{hash hideClear=true}}
    @baseQuery={{@baseQuery}}
  />
</span>