<li id={{this.dropdownElementId}} class="action-menu__item action-menu__dropdown-item" ...attributes {{did-insert this.elementInsertedInDOM}} {{will-destroy this.elementWillBeRemovedFromDOM}}>
  <a id={{this.ariaId}} class="action-menu__link action-menu__dropdown-trigger" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
    {{#if @iconComponent}}
      {{@iconComponent}}
    {{else}}
      <IconButtonMore />
    {{/if}}
  </a>
  <ul class="dropdown-menu dropdown-menu-right action-menu__dropdown-menu" aria-labelledby={{this.ariaId}}>
    {{yield}}
  </ul>
</li> 