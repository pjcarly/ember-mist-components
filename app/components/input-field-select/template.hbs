{{#if setSelectOptions.last}}
  {{#if isButtonGroup}}
    {{input-button-group selectOptions=cachedSelectOptions value=value valueChanged='valueChanged' styleClass=styleClass inputId=inputId}}
  {{else if isSelectSearch}}
    {{input-select-search selectOptions=cachedSelectOptions value=value valueChanged='valueChanged' styleClass=styleClass inputId=inputId}}
  {{else}}
    {{input-select selectOptions=cachedSelectOptions value=value showEmptyValue=showEmptyValue valueChanged='valueChanged' styleClass=styleClass inputId=inputId}}
  {{/if}}
{{/if}}
