{{#if this.setCountrySelectOptions.isIdle}}
  <OutputSelect
    @value={{@value}}
    @selectOptions={{this.selectOptions}}
    @prefix={{@prefix}}
    @suffix={{@suffix}}
    @class={{@class}}
    @options={{@options}}
    ...attributes
  />
{{else}}
  <span class="output loader">
    <Loading />
  </span>
{{/if}}