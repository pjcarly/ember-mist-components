{{#if this.value.countryCode}}
  {{#if this.value.format}}
    {{#each this.outputDisplayRows as |row|}}
      {{#unless row.emptyRow}}
        <div class="row">
          <div class="col-12">
            {{#each row.columns as |column|}}
              {{component
                (concat "output-" column.component)
                value=(get this.value column.field)
                valueChanged=(fn this.addressFieldChanged column.field)
                placeholder=column.label
                none=column.none
                selectOptions=column.selectOptions
                disabled=column.disabled
              }}
            {{/each}}
          </div>
        </div>
      {{/unless}}
    {{/each}}
  {{/if}}

  <div class="row">
    <div class="col-12">
      <OutputSelectCountry @value={{this.value.countryCode}} />
    </div>
  </div>
{{else}}
  <span class="output address">
    <span class="value empty"></span>
  </span>
{{/if}}