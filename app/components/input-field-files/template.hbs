<InputMultiField 
  @inputComponent="input-file-drupal"
  @value={{this.value}}
  @field={{@field}}
  @model={{@model}}
  @valueChanged={{action (mut this.value)}}
  @preSetHook={{@preSetHook}}
  @placeholder={{@placeholder}}
  @prefix={{@prefix}}
  @suffix={{@suffix}}
  @inputClass={{@inputClass}}
  @inputId={{@inputId}}
  @inputOptions={{this.inputOptions}}/>

